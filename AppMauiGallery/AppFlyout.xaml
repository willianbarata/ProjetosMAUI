<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:pages="clr-namespace:AppMauiGallery.Views"
             x:Class="AppMauiGallery.AppFlyout"
            FlyoutLayoutBehavior="Popover"
             Title="AppFlyout">
    <FlyoutPage.Flyout>
        <pages:Menu/>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <pages:MainPage/>
    </FlyoutPage.Detail>
    
</FlyoutPage>